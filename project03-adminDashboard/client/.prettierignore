# Ignore node_modules directory
node_modules/

# Ignore build output directories
dist/
build/

# Ignore static assets (images, fonts, etc.)
public/
*.jpg
*.jpeg
*.png
*.gif
*.svg
*.ico
*.woff
*.woff2
*.eot
*.ttf

# Ignore test coverage and log files
coverage/
*.log

# Ignore IDE-specific files
.vscode/
*.sublime-project
*.sublime-workspace
*.idea/

# Ignore other generated files
*.lock
*.tgz
*.gz
*.zip

# Ignore package lock and yarn lock files
package-lock.json
yarn.lock

# Ignore environment files
.env
.env.*